Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Punc3", "[", 
    RowBox[{"a_", ",", "b_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"IntegerQ", "[", 
      RowBox[{"a", "-", "b"}], "]"}], ",", 
     RowBox[{"a", "-", 
      RowBox[{"Floor", "[", "a", "]"}]}], ",", 
     RowBox[{
      RowBox[{"-", "a"}], "-", "b", "-", 
      RowBox[{"Floor", "[", 
       RowBox[{
        RowBox[{"-", "a"}], "-", "b"}], "]"}]}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.655927331885777*^9, 3.6559273503980837`*^9}, {
   3.655927382575884*^9, 3.655927388599864*^9}, {3.655927452036004*^9, 
   3.65592746276375*^9}, {3.6559299684706793`*^9, 3.655929969797477*^9}, 
   3.656016729928142*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"InDomain", "[", 
    RowBox[{"a1_", ",", "a2_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"IntegerQ", "[", 
       RowBox[{"a1", "-", "a2"}], "]"}], "&&", 
      RowBox[{"a2", "\[GreaterEqual]", 
       RowBox[{
        RowBox[{"-", "a1"}], "/", "2"}]}], "&&", 
      RowBox[{"a2", "\[LessEqual]", "a1"}], "&&", 
      RowBox[{"a2", "\[LessEqual]", 
       RowBox[{"k", "-", 
        RowBox[{"2", "a1"}]}]}]}], ",", "True", ",", "False"}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.656001755539415*^9, 3.6560018820151854`*^9}}],

