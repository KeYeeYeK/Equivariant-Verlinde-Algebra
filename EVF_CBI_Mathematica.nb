Cell[TextData[StyleBox["2.2 Index for SU(3) theory with 6 flavors", \
"Subsection"]], "Subsubsection",
 CellChangeTimes->{{3.655581422428187*^9, 3.655581426162812*^9}, {
  3.6561050860024347`*^9, 3.6561050933510838`*^9}, {3.672448841155455*^9, 
  3.672448841723398*^9}}],

Cell["\<\
We now compute the weakly coupled Lagrangian description of the dual \
theories. SU(3) gauge theory with 6 flavors of hypermultiplets is \
superconformal, and the Coulomb branch index comes from vectors and zero \
point energy of hypermultiplets.\
\>", "Text",
 CellChangeTimes->{{3.672447319340464*^9, 3.672447402318742*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SU3Vec", "[", 
    RowBox[{"m1_", ",", "m2_", ",", "m3_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"m1", "\[Equal]", "m2"}], "||", 
      RowBox[{
       RowBox[{"m1", "-", "m2"}], "\[Equal]", "k"}]}], ",", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"m2", "\[Equal]", "m3"}], "||", 
        RowBox[{
         RowBox[{"m2", "-", "m3"}], "\[Equal]", "k"}]}], ",", 
       FractionBox["1", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["t", "2"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["t", "3"]}], ")"}]}]], ",", 
       FractionBox["1", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "t"}], ")"}], 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["t", "2"]}], ")"}]}]]}], "]"}], ",", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"m1", "\[Equal]", "m3"}], "||", 
        RowBox[{
         RowBox[{"m1", "-", "m3"}], "\[Equal]", "k"}]}], ",", 
       FractionBox["1", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "t"}], ")"}], 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["t", "2"]}], ")"}]}]], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"m2", "\[Equal]", "m3"}], "||", 
          RowBox[{
           RowBox[{"m2", "-", "m3"}], "\[Equal]", "k"}]}], ",", 
         FractionBox["1", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "-", "t"}], ")"}], 
           RowBox[{"(", 
            RowBox[{"1", "-", 
             SuperscriptBox["t", "2"]}], ")"}]}]], ",", 
         FractionBox["1", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"1", "-", "t"}], ")"}], "2"]]}], "]"}]}], "]"}]}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.655581478547737*^9, 3.655581504750888*^9}, {
  3.655581878375745*^9, 3.655581910932478*^9}, {3.6555819895208*^9, 
  3.6555821554741507`*^9}, {3.655582296400099*^9, 3.6555822967265882`*^9}, {
  3.656000651922426*^9, 3.656000657102984*^9}, {3.6560006963288927`*^9, 
  3.656000700568369*^9}, {3.656038217637968*^9, 3.656038246815772*^9}, {
  3.656119116088839*^9, 3.6561191685651693`*^9}, {3.656168373469771*^9, 
  3.656168378498395*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ZV3", "[", 
    RowBox[{"m1_", ",", "m2_", ",", "m3_", ",", "k_"}], "]"}], ":=", 
   SuperscriptBox["t", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{"m1", "-", "m2"}], ",", "k"}], "]"}]}], "+", 
     RowBox[{
      FractionBox["1", "k"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"Mod", "[", 
         RowBox[{
          RowBox[{"m1", "-", "m2"}], ",", "k"}], "]"}], ")"}], "2"]}], "-", 
     RowBox[{"Mod", "[", 
      RowBox[{
       RowBox[{"m1", "-", "m3"}], ",", "k"}], "]"}], "+", 
     RowBox[{
      FractionBox["1", "k"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"Mod", "[", 
         RowBox[{
          RowBox[{"m1", "-", "m3"}], ",", "k"}], "]"}], ")"}], "2"]}], "-", 
     RowBox[{"Mod", "[", 
      RowBox[{
       RowBox[{"m2", "-", "m3"}], ",", "k"}], "]"}], "+", 
     RowBox[{
      FractionBox["1", "k"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"Mod", "[", 
         RowBox[{
          RowBox[{"m2", "-", "m3"}], ",", "k"}], "]"}], ")"}], "2"]}]}]]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.655582166442281*^9, 3.6555821993552513`*^9}, {
  3.6555822696294537`*^9, 3.655582433718856*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ZH3", "[", 
    RowBox[{"m_", ",", "n_", ",", "a_", ",", "k_"}], "]"}], ":=", 
   SuperscriptBox["t", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Mod", "[", 
        RowBox[{
         RowBox[{"m", "+", "n", "+", "a"}], ",", "k"}], "]"}], "-", 
       RowBox[{
        FractionBox["1", "k"], 
        SuperscriptBox[
         RowBox[{"Mod", "[", 
          RowBox[{
           RowBox[{"m", "+", "n", "+", "a"}], ",", "k"}], "]"}], "2"]}]}], 
      ")"}]}]]}], ";"}]], "Input",
 CellChangeTimes->{{3.6555826469741287`*^9, 3.6555826486843843`*^9}, {
