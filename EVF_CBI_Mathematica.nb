Cell[CellGroupData[{
Cell["\<\
Equivariant Verlinde Formula from Superconformal Index and Argyres-Seiberg \
duality\
\>", "Subchapter",
 CellChangeTimes->{{3.672445660133793*^9, 3.672445684706285*^9}}],

Cell[CellGroupData[{

Cell["\<\
                       S. Gukov,  D. Pei,  W. Yan  and  K. Ye\
\>", "Subsubsection",
 CellChangeTimes->{{3.672445692921898*^9, 3.672445744724717*^9}, {
  3.6736378573093357`*^9, 3.673637857746839*^9}}],

Cell[BoxData["\[IndentingNewLine]"], "Input",
 CellChangeTimes->{3.6724458192628527`*^9}],

Cell["\<\
In this Mathematica notebook we provide calculation of Coulomb branch Index \
on Lens spaces of class S theories. In section 1 we demonstrate the \
equivalence of the index with equivariant Verlinde formula for SU(2) gauge \
group. In section 2 we use Argyres-Seiberg duality to compute the equivariant \
Verlinde formula for SU(3) and show some properties of the moduli space of \
rank 3 Higgs bundle.\
\>", "Text",
 CellChangeTimes->{{3.6724458299008627`*^9, 3.672445995893585*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["1. SU(2) Case", "Section",
 CellChangeTimes->{{3.655298445061549*^9, 3.6552984484603853`*^9}, {
  3.6724458128766403`*^9, 3.6724458133311*^9}}],

Cell[BoxData[
 RowBox[{"Quit", "[", "]"}]], "Input",
 CellChangeTimes->{{3.655298476249302*^9, 3.6552984775446053`*^9}}],

Cell[TextData[{
 "Here we compute Lens space index of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "1"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "class S theory. We begin by listing the single particle index and its \
Coulomb branch limit, then we calculate the plethystic exponential explicitly."
}], "Text",
 CellChangeTimes->{{3.672446002605221*^9, 3.6724460859533443`*^9}}],

Cell[CellGroupData[{

Cell[TextData[StyleBox["1.1 Single particle index", "Subsection"]], \
"Subsubsection",
 CellChangeTimes->{{3.63836059771008*^9, 3.638360643921492*^9}, {
  3.638360691986507*^9, 3.638360696730131*^9}, {3.672448819078422*^9, 
  3.672448819907737*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SingleTrif", "[", 
    RowBox[{"\[Tau]_", ",", "\[Rho]_", ",", "\[Eta]_", ",", "m_", ",", "r_"}],
     "]"}], ":=", 
   RowBox[{
    FractionBox[
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"\[Tau]", " ", "\[Eta]"}], ")"}], "m"], 
       RowBox[{"1", "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Tau]", " ", "\[Eta]"}], ")"}], "r"]}]], "+", 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"\[Tau]", " ", "\[Rho]"}], ")"}], 
        RowBox[{"r", "-", "m"}]], 
       RowBox[{"1", "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Tau]", " ", "\[Rho]"}], ")"}], "r"]}]]}], 
     RowBox[{"1", "-", 
      RowBox[{
       SuperscriptBox["\[Tau]", "2"], "\[Rho]", " ", "\[Eta]"}]}]], 
    RowBox[{"(", 
     RowBox[{"\[Tau]", "-", 
      RowBox[{"\[Tau]", " ", "\[Rho]", " ", "\[Eta]"}]}], ")"}]}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.638792921307726*^9, 3.638793071434619*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SingleVect", "[", 
    RowBox[{"\[Tau]_", ",", "\[Rho]_", ",", "\[Eta]_", ",", "m_", ",", "r_"}],
     "]"}], ":=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Tau]", " ", "\[Eta]"}], ")"}], "m"], 
        RowBox[{"1", "-", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[Tau]", " ", "\[Eta]"}], ")"}], "r"]}]], "+", 
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Tau]", " ", "\[Rho]"}], ")"}], 
         RowBox[{"r", "-", "m"}]], 
        RowBox[{"1", "-", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[Tau]", " ", "\[Rho]"}], ")"}], "r"]}]]}], 
      RowBox[{"1", "-", 
       RowBox[{
        SuperscriptBox["\[Tau]", "2"], "\[Rho]", " ", "\[Eta]"}]}]], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SuperscriptBox["\[Tau]", "2"], "\[Rho]", " ", "\[Eta]"}], "+", 
       RowBox[{"\[Rho]", " ", "\[Eta]"}], "-", "1", "-", 
       SuperscriptBox["\[Tau]", "2"]}], ")"}]}], "+", 
    RowBox[{"KroneckerDelta", "[", 
     RowBox[{"m", ",", "0"}], "]"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.638793177371867*^9, 3.638793222566629*^9}, {
  3.638793276740205*^9, 3.638793316639929*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ZeroTrif", "[", 
    RowBox[{
    "\[Tau]_", ",", "\[Rho]_", ",", "\[Eta]_", ",", "m1_", ",", "m2_", ",", 
     "m3_", ",", "r_"}], "]"}], ":=", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"\[Rho]", " ", "\[Eta]"}], ")"}], 
    RowBox[{
     FractionBox["1", "4"], 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Mod", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"m1", " ", "s1"}], "+", 
           RowBox[{"m2", " ", "s2"}], "+", 
           RowBox[{"m3", " ", "s3"}]}], ",", "r"}], "]"}], "-", 
        RowBox[{
         FractionBox["1", "r"], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"Mod", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"m1", " ", "s1"}], "+", 
              RowBox[{"m2", " ", "s2"}], "+", 
              RowBox[{"m3", " ", "s3"}]}], ",", "r"}], "]"}], ")"}], "2"]}]}],
        ",", 
       RowBox[{"{", 
        RowBox[{"s1", ",", 
         RowBox[{"-", "1"}], ",", "1", ",", "2"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"s2", ",", 
         RowBox[{"-", "1"}], ",", "1", ",", "2"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"s3", ",", 
         RowBox[{"-", "1"}], ",", "1", ",", "2"}], "}"}]}], "]"}]}]]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.638793442813418*^9, 3.6387934492486877`*^9}, {
  3.638793540206235*^9, 3.6387935423423157`*^9}, {3.639766439277445*^9, 
  3.639766439882636*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ZeroVect", "[", 
    RowBox[{"\[Tau]_", ",", "\[Rho]_", ",", "\[Eta]_", ",", "m_", ",", "r_"}],
     "]"}], ":=", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"\[Rho]", " ", "\[Eta]"}], ")"}], 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{"2", "m"}], ",", "r"}], "]"}]}], "+", 
     RowBox[{
      FractionBox["1", "r"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"Mod", "[", 
         RowBox[{
          RowBox[{"2", "m"}], ",", "r"}], "]"}], ")"}], "2"]}]}]]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.638793595115758*^9, 3.638793617066155*^9}}]
