Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "2"], TraditionalForm]]],
 "class S theory, there is an additional type of puncture: the minimal \
puncture. When there is one minimal puncture along with two maximal puncture, \
one can think of the minimal puncture associated with a subspace of the full \
Hilbert space. Now we try to solve these minimal puncture, each as a \
combination of the base vectors in the Hilbert space."
}], "Text",
 CellChangeTimes->{{3.658362684765224*^9, 3.658362709994141*^9}, {
  3.6583689449863167`*^9, 3.658368971085442*^9}, {3.658369020266365*^9, 
  3.6583690285297737`*^9}, {3.672448476288221*^9, 3.672448533613332*^9}, {
  3.673637771227559*^9, 3.6736377741714563`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"V331", "[", 
    RowBox[{"a1_", ",", "a2_", ",", "b1_", ",", "b2_", ",", "u_", ",", "k_"}],
     "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"IntegerQ", "[", 
      RowBox[{"a1", "+", "b1", "+", "u"}], "]"}], ",", 
     RowBox[{
      RowBox[{"ZH3", "[", 
       RowBox[{"a1", ",", "b1", ",", "u", ",", "k"}], "]"}], 
      RowBox[{"ZH3", "[", 
       RowBox[{"a1", ",", "b2", ",", "u", ",", "k"}], "]"}], 
      RowBox[{"ZH3", "[", 
       RowBox[{"a1", ",", 
        RowBox[{
         RowBox[{"-", "b1"}], "-", "b2"}], ",", "u", ",", "k"}], "]"}], 
      RowBox[{"ZH3", "[", 
       RowBox[{"a2", ",", "b1", ",", "u", ",", "k"}], "]"}], 
      RowBox[{"ZH3", "[", 
       RowBox[{"a2", ",", "b2", ",", "u", ",", "k"}], "]"}], 
      RowBox[{"ZH3", "[", 
       RowBox[{"a2", ",", 
        RowBox[{
         RowBox[{"-", "b1"}], "-", "b2"}], ",", "u", ",", "k"}], "]"}], 
      RowBox[{"ZH3", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "a1"}], "-", "a2"}], ",", "b1", ",", "u", ",", "k"}], 
       "]"}], 
      RowBox[{"ZH3", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "a1"}], "-", "a2"}], ",", "b2", ",", "u", ",", "k"}], 
       "]"}], 
      RowBox[{"ZH3", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "a1"}], "-", "a2"}], ",", 
        RowBox[{
         RowBox[{"-", "b1"}], "-", "b2"}], ",", "u", ",", "k"}], "]"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"ZV3", "[", 
          RowBox[{"a1", ",", "a2", ",", 
           RowBox[{
            RowBox[{"-", "a1"}], "-", "a2"}], ",", "k"}], "]"}], 
         RowBox[{"ZV3", "[", 
          RowBox[{"b1", ",", "b2", ",", 
           RowBox[{
            RowBox[{"-", "b1"}], "-", "b2"}], ",", "k"}], "]"}]}], ")"}], 
       RowBox[{"1", "/", "2"}]]}], ",", "0"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6583690920484123`*^9, 3.658369117855015*^9}, {
  3.65836920544003*^9, 3.6583693499117327`*^9}, {3.6583695366882563`*^9, 
  3.658369560453454*^9}, {3.6583697681968603`*^9, 3.658369805864736*^9}, {
  3.658369900583414*^9, 3.65836990224747*^9}}],

