Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"CBIalg", "[", "k_", "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[ScriptCapitalI]H2", "[", 
       RowBox[{"m1", ",", "m2", ",", "m3", ",", "k"}], "]"}], 
      RowBox[{"\[ScriptCapitalI]V2", "[", 
       RowBox[{"m3", ",", "k"}], "]"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"ZeroVect", "[", 
          RowBox[{"0", ",", "\[Rho]", ",", "\[Eta]", ",", "m1", ",", "k"}], 
          "]"}], 
         RowBox[{"ZeroVect", "[", 
          RowBox[{"0", ",", "\[Rho]", ",", "\[Eta]", ",", "m2", ",", "k"}], 
          "]"}]}], ")"}], 
       RowBox[{"1", "/", "2"}]], 
      SuperscriptBox[
       RowBox[{"ZeroVect", "[", 
        RowBox[{"0", ",", "\[Rho]", ",", "\[Eta]", ",", "m3", ",", "k"}], 
        "]"}], 
       RowBox[{
        RowBox[{"-", "1"}], "/", "2"}]]}], ",", 
     RowBox[{"{", 
      RowBox[{"m1", ",", "0", ",", 
       RowBox[{"rsum", "[", "k", "]"}], ",", 
       RowBox[{"1", "/", "2"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m2", ",", "0", ",", 
       RowBox[{"rsum", "[", "k", "]"}], ",", 
       RowBox[{"1", "/", "2"}]}], "}"}], ",", 
